import java.util.Scanner;

public class C_prac3 {

	private static void printf(String string) {

		int num=5;
		Scanner s = new Scanner(System.in);
		for(int i=0;i<num;i++){
		    for(int j=num-1;j>i;j--){
		        printf(" ");
		    }
			    
		    for(int j=0;j<2*i+1;j++){
			printf("*");
		    }
		    printf("\n");
		}
	}


		
	}


