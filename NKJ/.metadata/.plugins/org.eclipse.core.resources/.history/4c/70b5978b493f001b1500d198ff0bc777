
public class Loop2 {

	public static void main(String[] args) {

		int hap = 0;
		for (int i = 0 ; i<10; i++) {
			hap = hap +(i+1);
			//hap += (1+1);
		}
		System.out.println("1~10±îÁö È¦¼öÇÕ1 :" + hap);
		
		for (int j=0; j<10 ; j++) {
			hap += (j+1);
		}
		System.out.println("1~10±îÁö È¦¼öÇÕ2 : " +hap);
	}

}
